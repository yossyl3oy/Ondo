{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Ondo",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:app:default",
    "core:app:allow-version",
    "shell:allow-open",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "process:allow-restart",
    "process:allow-exit"
  ]
}
